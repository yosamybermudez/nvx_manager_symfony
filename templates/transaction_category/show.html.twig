{% extends 'base.html.twig' %}

{% block title %}TransactionCategory{% endblock %}

{% block body %}
    <h1>TransactionCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transaction_category.id }}</td>
            </tr>
            <tr>
                <th>Uuid</th>
                <td>{{ transaction_category.uuid }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ transaction_category.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
