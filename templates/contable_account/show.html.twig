{% extends 'base.html.twig' %}

{% block title %}ContableAccount{% endblock %}

{% block body %}
    <h1>ContableAccount</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contable_account.id }}</td>
            </tr>
            <tr>
                <th>Uuid</th>
                <td>{{ contable_account.uuid }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ contable_account.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ contable_account.description }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ contable_account.createdAt ? contable_account.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ contable_account.updatedAt ? contable_account.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_contable_account_index') }}">back to list</a>

    <a href="{{ path('app_contable_account_edit', {'id': contable_account.id}) }}">edit</a>

    {{ include('contable_account/_delete_form.html.twig') }}
{% endblock %}
